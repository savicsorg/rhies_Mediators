{
  "name": "omrstonida-mediator",
  "version": "0.0.1",
  "description": "OpenMRS to NIDA mediator",
  "main": "lib/index.js",
  "scripts": {
    "start": "node lib/index.js",
    "test": "export NODE_ENV=test && npm -s run test:code && npm -s run test:style",
    "test:code": "tap --cov --reporter=spec test/basic.js",
    "test:style": "standard -v | snazzy",
    "cov": "npm run test:code -- --coverage-report=lcov",
    "test:watch": "onchange '**/*.js' -- npm test"
  },
  "keywords": [
    "openhim",
    "mediator"
  ],
  "author": "Savics",
  "license": "MPL-2.0",
  "contributors": [
    "Savics"
  ],
  "dependencies": {
    "express": "^4.13.4",
    "nconf": "0.10.0",
    "cron": "1.3.0",
    "needle": "^2.4.0",
    "openhim-mediator-utils": "^0.2.2",
    "request": "2.87.0",
    "urijs": "^1.17.1",
    "xmldom": "^0.1.22",
    "xpath": "0.0.23",
    "moment": "2.22.2",
    "formidable": "3.2.4"
  },
  "devDependencies": {
    "onchange": "^2.3.0",
    "winston": "^2.2.0",
    "rewire": "^2.5.1",
    "snazzy": "^3.0.1",
    "standard": "^6.0.8",
    "tap": "^5.7.1"
  }
}
